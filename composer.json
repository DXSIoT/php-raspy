{
    "name": "dxsiot/php-raspy",
    "description": "PHP Raspberry GPIO",
    "keywords": [
        "dxsiot",
        "php",
        "raspberry"
    ],
    "homepage": "https://github.com/DXSIoT/php-raspy",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DxsIoT\\RaspyPHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DxsIoT\\RaspyPHP\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
